.sousuo {
  margin-left: 629rpx;
  width: 15%;
  line-height: 150%;
  text-align: center;
  border: 1px rgb(240, 233, 233) solid;
  border-radius: 15rpx 15rpx 15rpx 15rpx;
  background-color: #F8F8F8;
  height:55rpx;
  justify-content: space-around; 
}
.icon{
  text-align:end;
  right: 0rpx;
  font-size: 28rpx;
}
.choose-merchant{
  width: 100%;
  height:487rpx;
  display: flex;
  flex-direction: column;
  border: 1rpx white solid;
  background:white;
  position:relative;
  z-index:2;
  overflow-y: auto;
  box-sizing: border-box;
  border-top: 0;
  transition: height 0.3s;
}
.choose-merchant view{
  background-color: white;
}
.choose-merchant view:hover{
  background-color: gainsboro;
}
.option{
  display: block;
  line-height: 35rpx;
  font-size: 32rpx;
  padding: 6rpx;
  overflow-x: hidden;
}
.page-section{
  width: 100%;
  margin-top: 10rpx;
}

.img_item {
  width: 49%;
  margin-left: 0.5%;
  margin-right: 0.5%;
  display: inline-block;
  vertical-align: top;
}
.title{
  margin-top: 5rpx; 
  float: left;
  height: 50%;
  overflow:hidden;
  white-space:nowrap; 
  text-overflow:ellipsis;
}
.buttom{
  font-size: 26rpx;
  width:100%;
  height: 90rpx;
  flex-direction: column;
  margin-top: -90rpx;
  margin-left: 10rpx;
  position: relative;
  color: white; 
}
.videodes{
  display: flex;
  flex-direction: row;
  font-weight:100;
  width: 99%;
  height: 50%;
  justify-content: space-between;
  align-items: center;
}
.username{
  width: 45%;
  overflow:hidden;
  white-space:nowrap; 
  text-overflow:ellipsis;
}
.loading{
  padding: 10rpx;
  text-align: center;
}
.loading:before{
  display: inline-block;
  margin-right: 5rpx;
  vertical-align: middle;
  content: '';
  width: 40rpx;
  height: 40rpx;
  background-size: contain;
  animation: rotate 1s linear infinite;
}
.loading.complete:before{
  display: none;
}
.addfile{
  width:100rpx;
  height:100rpx;
  bottom:100rpx;
  z-index:999;
  position:absolute;
  right:20rpx;
}
.videolist{
  margin-top: 20rpx;
  margin-left: 0rpx;
  margin-right: 10rpx;
}
