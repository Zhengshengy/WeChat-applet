.body{
  width:100%;
  overflow:hidden;
}
.flower{
  background-color:ghostwhite;
  border: 0.5rpx ghostwhite solid;
  width: 180rpx;
  height:60rpx;
  font-size: 30rpx;
  color: red;
  margin-right: 0rpx;
  margin-bottom: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.hide{
display: none;
}
.show{
  display: block;
}
.chat{
  margin-top:0;
  padding-right:20px;
  display:flex;
  flex-flow: row;
  justify-content:flex-end;
}
.chatlogo{
  width:60rpx;
  height:60px;
}
.unread{
  font-size: 80rpx;
  color: red;
  margin-top:-31rpx;
  margin-left:-10rpx;
}
.user-head {
  background:#fff;
  position: relative;
  display:-webkit-flex;
	display:flex;
  flex-flow: row;
	width:100%;
  height:300rpx;
  justify-content:center;
  align-content:center;
  margin-top: -15rpx;
}
.user-head .center{
    display: flex;
    flex-flow: row;
    padding-top: 15rpx;
    background-color:#FFE605;
    border-radius: 2%;
    align-items: center;
    width: 90%;
    
}
.user-img{
  overflow:hidden;
  display: block;
  width: 250rpx;
  height: 150rpx;
  margin-left: 50rpx;
  border-radius: 50%;
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}
.user-img image{
  width: 100%;
  height: 100%;
}
.user-text{
  margin-left: 100rpx;
  text-align:left;
  width:100%;
  font-size: 35rpx;
}
.user-text text{
  font-family: 微软雅黑;
  color:#333;
  font-size: 40rpx;
  margin-top: 30rpx;
}
.phone{
  margin-top: 35rpx;
}
.bianji{
  width:50rpx;
  height:50rpx;
  margin-top: -220rpx;
  margin-right: 20rpx;
}
.list {
  margin-top: 20rpx;
  background: #fff;
  display: flex;
  flex-flow: row;
  justify-content:space-between;
}
.list .item1{
  font-size: 14px;
  width: 33%;
  color:#333;
  position: relative;
  border-right: 1px solid #f3f3f3;
  height:100rpx;
  display: flex;
  flex-flow: column;
 justify-content:center;
  align-items: center
}
.list .item1::last-child{
  border:none;
}
.myitem{
   font-size: 16px;
   color:#333;
   position: relative;
   top:-10rpx;
}
.myitems{
   font-size: 14px;
   color:gray;
   position: relative;
   top:-20rpx;
}

::-webkit-scrollbar {
     display: none;
}
